  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MBehtemam/Sports-Poll-Backend/issues"
  },
  "homepage": "https://github.com/MBehtemam/Sports-Poll-Backend#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "graphql": "^14.1.1",
    "graphql-cli": "^3.0.11",
    "graphql-yoga": "^1.18.1",
    "jsonwebtoken": "^8.5.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prisma": "^1.27.1",
    "prisma-binding": "^2.3.3",
    "stripe": "^6.25.1"
