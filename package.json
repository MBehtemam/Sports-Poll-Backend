{
  "name": "sports-poll-backend",
  "version": "1.0.0",
  "description": "simple sports poll backend",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -e js,graphql -x node index.js",
    "debug": "nodemon -e js,graphql -x node --inspect index.js",
    "test": "jest",
    "playground": "graphql playground --dotenv variables.env",
    "dev": "npm-run-all --parallel debug playground",
    "deploy": "prisma deploy  --env-file variables.env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MBehtemam/Sports-Poll-Backend.git"
  },
